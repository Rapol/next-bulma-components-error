{
  "extends": ["airbnb", "plugin:jsx-a11y/recommended"],
  "plugins": ["react", "jsx-a11y"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "react/react-in-jsx-scope": 0
  }
}
